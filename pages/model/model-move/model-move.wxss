.map-view-container {
	box-sizing: border-box;
	background: #F8F8F7;
	width: 100%;
}

.map-view-content {
	background: #FFF;
	padding: 20px 0 0px;
	margin-top: 10px;
	height: 100%;
}

.map-container {
	display: flex;
	justify-content: center;
}

.map {
	width: 100%;
	height: 400px;
	position: relative;
}
  .active {
      color: #222;
      font-weight: 500;
  }

  .bottom-container {
    margin-top: 30px;
	padding-bottom: 60px;
}

.primary-btn-style {
	padding: 0px;
	margin: 0 auto;
	text-align: center;
	line-height: 46px;
	cursor: pointer;
	color: #FFF;
	font-size: 16px;
	border-radius: 4px;
	background: #3875FF;
	height: 46px;
	width: 198px;
	font-weight: 500;
}
.feature-content input{
    width: 85px;
    text-align: right;
}
.is-disabled {
    background: rgba(17, 17, 17, 0.1);
    color: rgba(34, 34, 34, 0.3)
}
